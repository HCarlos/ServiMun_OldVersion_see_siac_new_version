{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ext-openssl": "*",
        "doctrine/dbal": "^2.13.7",
        "elibyy/tcpdf-laravel": "^7.0.0",
        "facade/ignition": "^2.17.4",
        "fakerphp/faker": "^1.19",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.5",
        "intervention/image": "^2.7.1",
        "laravel-lang/lang": "~7.0.9",
        "laravel/framework": "^7.30.6",
        "laravel/legacy-factories": "^1.3.1",
        "laravel/socialite": "^4.4.1",
        "laravel/tinker": "^2.7.0",
        "laravel/ui": "^2.5",
        "laravelcollective/html": "^6.2.1",
        "laravelcollective/remote": "^6.2",
        "maatwebsite/excel": "^3.1.35",
        "opsway/doctrine-dbal-postgresql": "^0.8.1",
        "php-parallel-lint/php-console-color": "^0.3.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "phpoffice/phpspreadsheet": "^1.21.0",
        "picqer/php-barcode-generator": "^2.2",
        "predis/predis": "^1.1.10",
        "rapidapi/rapidapi-connect": "^0.0.2",
        "setasign/fpdf": "^1.8.4",
        "spatie/laravel-backup": "^6.16.5",
        "spatie/laravel-permission": "^3.18",
        "tecnickcom/tc-lib-pdf-font": "^1.9.6",
        "yajra/laravel-datatables-oracle": "^9.19.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^9.5.13"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan config:cache",
            "@php artisan cache:clear",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan route:clear",
            "@php artisan route:clear"
        ],
        "post-install-cmd": [
            "@php artisan config:cache",
            "@php artisan cache:clear",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan route:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
